{
	"schemaVersion": 1,
	"id": "stick",
	"version": "${version}",
	"name": "Stick Mod",
	"description": "Adds 'sticks' to Minecraft",
	"authors": [
		"Hootisman"
	],
	"contact": {
		"homepage": "https://fabricmc.net/",
		"sources": "https://github.com/FabricMC/fabric-example-mod"
	},
	"license": "CC0-1.0",
	"icon": "assets/stick/icon.png",
	"environment": "*",
	"entrypoints": {
		"main": [
			{
				"value": "io.github.hootisman.Stick",
				"adapter": "kotlin"
			}
		],
		"client": [
			{
				"value": "io.github.hootisman.StickClient",
				"adapter": "kotlin"
			}
		],
		"fabric-datagen": [
			{
				"value": "io.github.hootisman.StickDataGenerator",
				"adapter": "kotlin"
			}
		]
	},
	"mixins": [
		"stick.mixins.json",
		{
			"config": "stick.client.mixins.json",
			"environment": "client"
		}
	],
	"depends": {
		"fabricloader": ">=0.14.24",
		"minecraft": "~1.20.2",
		"java": ">=17",
		"fabric-api": "*",
		"fabric-language-kotlin": ">=1.9.20"
	},
	"suggests": {
		"another-mod": "*"
	}
}